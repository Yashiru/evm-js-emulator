{
    "compilerOptions": {
        "module": "commonjs",
        "declaration": true,
        "removeComments": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "allowSyntheticDefaultImports": true,
        "target": "es2020",
        "esModuleInterop": true,
        "keyofStringsOnly": false,
        "sourceMap": true,
        "skipLibCheck": true,
        "outDir": "./dist",
        "incremental": true,
        "strict": true,
        "resolveJsonModule": true,
        "types": [],
        "paths": {
        }
    }
}
